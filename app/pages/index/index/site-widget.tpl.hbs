<div class="panel-heading" id="{{id}}-header">
  <span class="pull-right">
    <i class="fa" id="{{id}}-icon"></i>
  </span>

  <h3 class="panel-title" id="{{id}}-name">{{data.name}}</h3>
</div>
<ul class="list-group module-list" id="{{id}}-module-list">
  <li class="list-group-item text-center"><i class="fa fa-spiner fa-spin"></i> Loading modules...</li>
</ul>

<div class="panel-footer">
  <div class="btn-group pull-right">
    <button class="btn btn-xs btn-default" id="{{id}}-edit"><i class="fa fa-edit"></i> <span class="hidden-xs">Edit</span>
    </button>
    <button class="btn btn-xs btn-default" id="{{id}}-show-modules"><i class="fa fa-tasks"></i> <span class="hidden-xs">Modules</span>
    </button>
  </div>

  <button class="btn btn-xs btn-success" disabled id="{{id}}-run"><i class="fa fa-play"></i> <span class="hidden-xs">Run</span>
  </button>
</div>
